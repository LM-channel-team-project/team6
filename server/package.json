{
  "name": "server",
  "version": "1.0.0",
  "main": "app.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^8.0.29",
    "@types/passport": "^1.0.6",
    "@types/passport-github": "^1.1.5",
    "@types/passport-kakao": "^0.2.0",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "3.3.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.8.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.10",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.32",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2"
  },
  "scripts": {
    "prebuild": "tsc -b --clean",
    "build": "tsc",
    "docs": "swagger-cli bundle ./src/utils/swagger/swagger.yaml --outfile build/swagger.yaml --type yaml",
    "start:dev": "yarn build && yarn docs && cross-env NODE_ENV=development nodemon --exec ts-node src/app.ts",
    "start:prod": "yarn build && yarn docs && cross-env NODE_ENV=production ts-node build/app.js",
    "test": "cross-env NODE_ENV=test && jest --detectOpenHandles --forceExit"
  },
  "_moduleAliases": {
    "@controllers": "build/controllers",
    "@middlewares": "build/middlewares",
    "@models": "build/models",
    "@routes": "build/routes",
    "@utils": "build/utils"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
